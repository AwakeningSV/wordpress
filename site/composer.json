{
  "name": "roots/bedrock",
  "type": "project",
  "license": "UNLICENSED",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "composer", "roots", "wordpress", "wp", "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
       "type": "vcs",
       "url": "https://github.com/awakening-church/ApplicationInsights-WordPress.git"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/azure-cdn-rewrite-plugin/azure-cdn-rewrite-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/remove-socialize-plugin/remove-socialize-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/live-plugin/live-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/teaching-plugin/teaching-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/events-calendar-pro/events-calendar-pro"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/upcoming-events-plugin/upcoming-events-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/oembed-image-plugin/oembed-image-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/leadership-plugin/leadership-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/wpbakery-page-builder-plugin/wpbakery-page-builder-plugin"
    },
    {
      "type": "vcs",
      "url": "git@ssh.dev.azure.com:v3/awakeningsv/revslider/revslider"
    }
  ],
  "require": {
    "php": ">=7.1",
    "awakening-church/applicationinsights-wordpress": "dev-master",
    "awakeningsv/azure-cdn-rewrite-plugin": "dev-master",
    "awakeningsv/remove-socialize-plugin": "dev-master",
    "awakeningsv/oembed-image-plugin": "dev-master",
    "awakeningsv/live-plugin": "dev-master",
    "awakeningsv/teaching-plugin": "dev-master",
    "awakeningsv/leadership-plugin": "dev-master",
    "awakeningsv/upcoming-events-plugin": "dev-master",
    "awakeningsv/revslider": "dev-master",
    "composer/installers": "^1.4",
    "vlucas/phpdotenv": "^3.0.0",
    "oscarotero/env": "^1.1.0",
    "roots/wordpress": "^5.4",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/accesspress-social-icons": "^1.7",
    "wpackagist-plugin/akismet": "^4.1",
    "wpackagist-plugin/autodescription": "^4.0",
    "wpackagist-plugin/fluid-video-embeds": "^1.2",
    "wpackagist-plugin/page-excerpt": "^1.3",
    "wpackagist-plugin/manual-image-crop": "^1.12",
    "wpackagist-plugin/powerpress": "^8.0",
    "wpackagist-plugin/reftagger": "^2.3",
    "wpackagist-plugin/regenerate-thumbnails": "^3.1",
    "wpackagist-plugin/zoninator": "^0.8.0",
    "wpackagist-plugin/quick-featured-images": "^13.3",
    "wpackagist-plugin/windows-azure-storage": "^4.1",
    "wpackagist-plugin/wpcustom-category-image": "^2.1",
    "wpackagist-plugin/post-type-converter": "^0.6.0",
    "wpackagist-plugin/oa-social-login": "^5.5",
    "wpackagist-plugin/simple-history": "^2.33",
    "wpackagist-plugin/redirection": "^4.4",
    "wpackagist-plugin/wp-webhooks": "^1.1",
    "wpackagist-plugin/getwid": "^1.5"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2",
    "roave/security-advisories": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
